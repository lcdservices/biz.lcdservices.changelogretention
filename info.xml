<?xml version="1.0"?>
<extension key="biz.lcdservices.changelogretention" type="module">
  <file>changelogretention</file>
  <name>Changelog retention/purging</name>
  <description>Creates an API that can be run as a scheduled job to purge changelog records outside of the configured retention window.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Lighthouse Consulting and Design</author>
    <email>dev@lcdservices.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/lcdservices/biz.lcdservices.changelogretention</url>
    <url desc="Documentation">https://github.com/lcdservices/biz.lcdservices.changelogretention/blob/master/README.md</url>
    <url desc="Support">https://github.com/lcdservices/biz.lcdservices.changelogretention/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-04-07</releaseDate>
  <version>2.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/LCD/changelogretention</namespace>
    <format>24.09.0</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_LCD_changelogretention_Upgrader</upgrader>
</extension>
